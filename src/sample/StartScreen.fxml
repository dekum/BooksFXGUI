<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="260.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.StartScreenController">
   <children>
      <Label layoutX="13.0" layoutY="49.0" text="Welcome to BookFX GUI">
         <font>
            <Font name="System Bold" size="37.0" />
         </font>
      </Label>
      <Label layoutX="27.0" layoutY="147.0" text="Press &quot;Get Started&quot; to Continue or &quot;Quit&quot; to exit">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <Button layoutX="293.0" layoutY="232.0" mnemonicParsing="false" onAction="#handleStart" text="Get Started" />
      <Button fx:id="btnExit" layoutX="422.0" layoutY="232.0" mnemonicParsing="false" onAction="#handleQuit" text="Quit" />
   </children>
</AnchorPane>
